import { Module } from '@nestjs/common';
import { MongooseModule } from '@nestjs/mongoose';
<% if (controller) { %>import { <%= nameUpper %>Controller } from './<%= controllerFileName %>';<% } 
%><% if (service) { %>import { <%= nameUpper %>Service } from './<%= serviceFileName %>';<% } 
%>import { <% if (old) { %><%= nameUpperCase %>_MODEL<% } else { %><%= nameUpper %><% } -%>, <%= nameUpper %>Schema } from './schemas/<%= name %>.schema';

@Module({
  imports: [
    MongooseModule.forFeature([
      {
        name: <% if (old) { %><%= nameUpperCase %>_MODEL<% } else { %><%= nameUpper %>.name<% } -%>,
        schema: <%= nameUpper %>Schema,
      },
    ]),
  ],
  controllers: [<% if (controller) { %><%= nameUpper %>Controller<% } -%>],
  providers: [<% if (service) { %><%= nameUpper %>Service<% } -%>],
})
export class <%= nameUpper %>Module {}
